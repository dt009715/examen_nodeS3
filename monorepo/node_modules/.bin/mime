#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/diego/Documents/3WA/exam_nodeS3/monorepo/node_modules/mime/node_modules:/mnt/c/Users/diego/Documents/3WA/exam_nodeS3/monorepo/node_modules:/mnt/c/Users/diego/Documents/3WA/exam_nodeS3/node_modules:/mnt/c/Users/diego/Documents/3WA/node_modules:/mnt/c/Users/diego/Documents/node_modules:/mnt/c/Users/diego/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/diego/Documents/3WA/exam_nodeS3/monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/diego/Documents/3WA/exam_nodeS3/monorepo/node_modules/mime/node_modules:/mnt/c/Users/diego/Documents/3WA/exam_nodeS3/monorepo/node_modules:/mnt/c/Users/diego/Documents/3WA/exam_nodeS3/node_modules:/mnt/c/Users/diego/Documents/3WA/node_modules:/mnt/c/Users/diego/Documents/node_modules:/mnt/c/Users/diego/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/diego/Documents/3WA/exam_nodeS3/monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
